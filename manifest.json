{
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "browser_action": {
    "default_icon": "image/onglet.png"
  },
  "manifest_version": 2,
  "name": "Tab Manager",
  "description": "This extension allows the user to manage the tab.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage"
  ],
  "commands": {
    "toggle-feature": {
      "suggested_key": {
        "default": "Ctrl+Shift+E",
        "mac": "Command+Shift+E"
      },
      "description": "activate"
    }
  }
}
